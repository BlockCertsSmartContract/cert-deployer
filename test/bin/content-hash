#!/Users/florianweiss/Desktop/PASGIT/cert-deployer/test/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'content-hash==1.1.1','console_scripts','content-hash'
__requires__ = 'content-hash==1.1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('content-hash==1.1.1', 'console_scripts', 'content-hash')()
    )
